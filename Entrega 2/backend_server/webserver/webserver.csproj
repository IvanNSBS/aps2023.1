<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>

    <OutputPath>$(SolutionDir)output/webserver/bin</OutputPath>
    <BaseIntermediateOutputPath>$(SolutionDir)output/webserver/obj</BaseIntermediateOutputPath>
    <MSBUildProjectExtensionsPath>$(SolutionDir)output/webserver/bin</MSBUildProjectExtensionsPath>
    
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.5.0" />
  </ItemGroup>

</Project>
